{
  "name": "koa-health-user-activity-manager",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "dev": "nodemon --watch \"src/**\" --ext \"ts,json\" --ignore \"src/**/*.spec.ts\" --exec  \"node -r ts-node/register/transpile-only src/api/server.ts\"",
    "lint:fix": "eslint --fix ./src",
    "format": "prettier --write .",
    "migration:create": "node -r ts-node/register/transpile-only src/scripts/migrate create --name",
    "migration:up": "node -r ts-node/register/transpile-only src/scripts/migrate up",
    "migration:down": "node -r ts-node/register/transpile-only src/scripts/migrate down",
    "migration:repair": "node -r ts-node/register/transpile-only src/scripts/migrate repair",
    "seed:activity": "node -r ts-node/register/transpile-only src/scripts/seeds/seed-activity-table.ts",
    "test": "NODE_ENV=test jest --runInBand --verbose --coverage --detectOpenHandles",
    "test:watch": "NODE_ENV=test jest --runInBand --verbose --watchAll --detectOpenHandles",
    "test:migration:up": "NODE_ENV=test npm run migration:up",
    "test:migration:down": "NODE_ENV=test npm run migration:down",
    "test:migration:repair": "NODE_ENV=test npm run migration:repair"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@pgkit/client": "^0.1.9",
    "@pgkit/migrator": "^0.1.9",
    "bcrypt": "^5.1.1",
    "celebrate": "^15.0.3",
    "config": "^3.3.11",
    "cors": "^2.8.5",
    "express": "^4.19.2",
    "helmet": "^7.1.0",
    "joi": "^17.12.3",
    "jsonwebtoken": "^9.0.2",
    "pino": "^8.20.0",
    "pino-http": "^9.0.0",
    "ramda": "^0.29.1",
    "supertest": "^6.3.4",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.0.2",
    "@eslint/js": "^9.0.0",
    "@types/bcrypt": "^5.0.2",
    "@types/config": "^3.3.4",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.12",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/node": "^20.12.7",
    "@types/ramda": "^0.29.12",
    "@types/supertest": "^6.0.2",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.6",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "eslint": "^8.57.0",
    "eslint-config-standard-with-typescript": "^43.0.1",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-n": "^16.6.2",
    "eslint-plugin-promise": "^6.1.1",
    "globals": "^15.0.0",
    "jest": "^29.7.0",
    "nodemon": "^3.1.0",
    "prettier": "^3.2.5",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.4.5",
    "typescript-eslint": "^7.6.0"
  }
}
